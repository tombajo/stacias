using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.RegularExpressions;

namespace Rextester
{
    public class Program
    {
        public static void Main(string[] args)
        {
            int ilgis = 5;
            int plotis = 3;
            int x1 = 1;
            int y1 = 4;
            int x2, x3, x4;
            int y2,y3,y4;
            int plotas, perimetras;
            double istrizaine;
            
            plotas = ilgis * plotis;
            perimetras = (2* ilgis) + (2*plotis);
            istrizaine = Math.Sqrt(x1*x1 + y1*y1);
            
            x2 = x1 + ilgis;
            y2 = y1;
            x3 = x1;
            y3 = y1 - plotis;
            x4 = x1 + ilgis;
            y4 = y1 - plotis;
            
            Console.WriteLine("Duotojo staciakampio plotis: " + plotis);
            Console.WriteLine("Duotojo staciakampio ilgis: " + ilgis);
            Console.WriteLine("Duotojo staciakampio pirmoji koordinate x = " + x1 + " ,y koordinate = " + y1 );
            Console.WriteLine("Apskaiciavome : ");
            Console.WriteLine("Staciakampio plotas: " + plotas);
            Console.WriteLine("Staciakampio perimetras: " + perimetras);
            Console.WriteLine("Staciakampio istrizaines ilgis: " + istrizaine);
            Console.WriteLine("Duotojo staciakampio antroji koordinate x = " + x2 + " ,y koordinate = " + y2 );
            Console.WriteLine("Duotojo staciakampio trecioji koordinate x = " + x3 + " ,y koordinate = " + y3 );
            Console.WriteLine("Duotojo staciakampio ketvirtoji koordinate x = " + x4 + " ,y koordinate = " + y4 );
            
           
            
            

            
        }
    }
}
